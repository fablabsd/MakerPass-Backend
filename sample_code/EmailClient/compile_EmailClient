g++ -I/usr/local/include/Poco  -Wall -Wno-sign-compare -DPOCO_BUILD_HOST=raspberrypi  -D_XOPEN_SOURCE=500 -D_REENTRANT -D_THREAD_SAFE -D_FILE_OFFSET_BITS=64 -D_LARGEFILE64_SOURCE -DPOCO_HAVE_FD_EPOLL  -O2 -fpermissive -DNDEBUG -fPIC  -c ./EmailClient.cpp  -o ./EmailClient.o


echo g++ -O2 -Wl,-rpath,/usr/local/lib -o ./EmailClient ./EmailClient.o -L/usr/local/lib  -lPocoUtil -lPocoNet -lPocoXML -lPocoFoundation -lpthread -ldl -lrt

